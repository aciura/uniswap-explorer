{
  "version": 2,
  "env": {
    "ETHEREUM_NETWORK": "mainnet",
    "INFURA_PROJECT_ID": ""
  },
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": [
        "GET"
      ]
    }
  ]
}